<?php
// DB연결
include 'pdo.php';

// DB조회
$sql = "SELECT sUserEmail, sNickName
        FROM tUserList;";

// DB 리스트 꺼내오기
foreach($connect->query($sql) as $field) {
    echo '<tr>'.'<td>'.$field['nUserEmail'].'</td>';
    echo '<td>'.$field['sNickName'].'</td>'.'</tr>';
}

mysqli_close($db);

